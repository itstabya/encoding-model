plot(tw1, 'r')
hold on
plot(tw2, 'b')
title('Level 2 Difficulty')
xlabel('Timeframe')
ylabel('Neural Activity')
ylim([-1, 2])

figure(11)
plot(th1, 'r')
hold on
plot(th2, 'b')
title('Level 3 Difficulty')
xlabel('Timeframe')
ylabel('Neural Activity')
ylim([-1, 2])

figure(12)
plot(f1, 'r')
hold on
plot(f2, 'b')
title('Level 4 Difficulty (Easiest)')
xlabel('Timeframe')
ylabel('Neural Activity')
ylim([-1, 2])

%% GENERAL PLOTS OF CORRECT AND INCORRECT
figure(13)
plot(rew1, 'r')
hold on
plot(rew2, 'b')
title('Neural Activity from Correct Trials')
ylim([-1, 2.5])

figure(14)
plot(w1, 'r')
hold on
plot(w2, 'b')
title('Neural Activity from Incorrect Trials')
ylim([-1, 2.5])

