%%  GENERAL PLOTS OF LEFT AND RIGHT

figure(7)
plot(l1, 'r')
hold on
plot(l2, 'b')
title('Neural Activity from Left Stim')
xlabel('Timeframe')
ylabel('Neural Activity')
ylim([-1, 2])

figure(8)
plot(r1, 'r')
hold on
plot(f2, 'b')
title('Neural Activity from Right Stim')
xlabel('Timeframe')
ylabel('Neural Activity')
ylim([-1, 2])

